PROJECT_NAME     = "Intel® HE transformer for nGraph™"
PROJECT_BRIEF    = "Intel® HE transformer for nGraph™"

OUTPUT_DIRECTORY = @CMAKE_CURRENT_BINARY_DIR@
INPUT            = @CMAKE_SOURCE_DIR@/src
RECURSIVE        = YES
EXCLUDE_PATTERNS = json.hpp

USE_MATHJAX      = YES

GENERATE_XML     = YES
EXTRACT_ALL      = YES
SHOW_NAMESPACES  = YES

WARN_IF_UNDOCUMENTED = @DOXYGEN_WARN_IF_UNDOCUMENTED@
WARN_IF_DOC_ERROR     = YES
WARN_NO_PARAMDOC      = YES

QUIET                = @DOXYGEN_QUIET@